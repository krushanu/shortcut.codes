sudo apt update
sudo apt install vim git build-essential zsh powerline fonts-powerline curl ubuntu-restricted-extras ubuntu-restricted-addons fonts-inconsolata ffmpeg ffmpegthumbnailer
ssh-keygen -t ed25519
git config --global user.email "willfong@pm.me"
git config --global user.name "Will Fong"
sh -c "$(curl -fsSL https://raw.githubusercontent.com/ohmyzsh/ohmyzsh/master/tools/install.sh)"

sudo update-locale LANG=C.UTF-8
sudo update-locale LC_ALL=C.UTF-8

sudo snap install code --classic
sudo snap install node --channel=12/stable --classic
sudo snap install spotify

